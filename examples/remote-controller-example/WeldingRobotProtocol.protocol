typestate WeldingRobotProtocol {
  OFF = {
    void turnOn(): IDLE,
    drop: end
  }

  IDLE = {
    void turnOff(): OFF,
    boolean move(double, double): <true: IDLE, false: RECHARGE>,
    boolean weldMetal(): <true: IDLE, false: HEAT>
  }

  RECHARGE = {
    void turnOff(): OFF,
    void recharge(): IDLE
  }

  HEAT = {
    void turnOff(): OFF,
    void heating(): IDLE
  }
}
