typestate StackProtocol {

  EMPTY = {
    void push(Robot): NON_EMPTY,
    boolean isEmpty(): <true: EMPTY, false: NON_EMPTY>,
    drop: end
  }

  NON_EMPTY = {
     void push(Robot): NON_EMPTY,
     Robot pop(): UNKNOWN,
     boolean isEmpty(): <true: EMPTY, false: NON_EMPTY>
  }

  UNKNOWN = {
    void push(Robot): NON_EMPTY,
    boolean isEmpty(): <true: EMPTY, false: NON_EMPTY>
  }

}
