import org.checkerframework.checker.jtc.lib.State;

package java.lang;

public interface Iterable<E> {
  @State("HasNext") Iterator<E> iterator();
}

package java.util;

public interface Collection<E> {
  @State("HasNext") Iterator<E> iterator();
}

public interface List<E> {
  @State("HasNext") Iterator<E> iterator();
}
